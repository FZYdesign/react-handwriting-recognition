{
  "name": "react-handwriting-recognition",
  "version": "1.0.0",
  "description": "React component with handwriting recognition provided by MyScript",
  "private": true,
  "main": "lib/index.js",
  "scripts": {
    "start": "NODE_ENV=dev webpack-dev-server -d --colors --hot --inline --progress",
    "bundle": "NODE_ENV=production webpack -p --colors --progress",
    "lib": "babel src --out-dir lib",
    "build": "npm run lib && npm run bundle"
  },
  "author": "Mathieu ANCELIN",
  "license": "MIT",
  "dependencies": {
    "cryptojs": "^2.5.3",
    "handjs": "^1.3.11",
    "q": "^1.4.1",
    "react": "^0.14.3",
    "react-dom": "^0.14.3"
  },
  "devDependencies": {
    "babel": "^6.1.18",
    "babel-cli": "^6.2.0",
    "babel-core": "^6.2.1",
    "babel-loader": "^6.2.0",
    "babel-preset-es2015": "^6.1.18",
    "babel-preset-react": "^6.1.18",
    "babel-preset-stage-0": "^6.1.18",
    "webpack": "^1.12.9",
    "webpack-dev-server": "^1.14.0"
  }
}
